{
  "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --no-input",
  "builds": [
    {
      "src": "pweb/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "pweb/wsgi.py"
    }
  ]
}